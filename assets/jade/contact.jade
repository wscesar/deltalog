section#contact
	.wrap
		img(src="assets/img/logo.svg", width="266", height="37").logo
		
		form(onsubmit="send_form();  return false")
			.col1
				div
					input(type="text", id="name", name="name", required).text
					label(for="name") Nome

				div
					input(type="email", id="email", name="email", required).text
					label(for="email") Email

				div
					input(type="tel", id="phone", name="phone", required).text
					label(for="phone") Telefone


			.col2
				div
					textarea(id="msg", name="msg", required).text
					label(for="msg") Mensagem

				div
					button.submit enviar